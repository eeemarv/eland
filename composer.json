{
	"name": "eeemarv/eland",
	"license": "AGPL-3.0",
	"type": "project",
	"description": "web app for community currencies.",
	"homepage": "https://eland.letsa.net",
	"keywords": ["community", "currencies", "app"],

    "require": {
		"php": "^8.1",
		"ext-ctype": "*",
		"ext-exif": "*",
		"ext-gd": "*",
		"ext-iconv": "*",
		"ext-intl": "*",
		"ext-mbstring": "*",
		"ext-redis": "*",
		"ext-xml": "*",
		"ext-zip": "*",
		"aws/aws-sdk-php": "^3",
		"bramus/monolog-colored-line-formatter": "^2",
		"composer/package-versions-deprecated": "1.11.99.1",
		"doctrine/dbal": "^3",
		"doctrine/doctrine-bundle": "^2",
		"doctrine/doctrine-migrations-bundle": "^3",
		"doctrine/orm": "^2",
		"ezyang/htmlpurifier": "^4.8",
		"geo6/geocoder-php-bpost-provider": "^1.4",
		"gregwar/captcha": "^1.1",
		"gregwar/captcha-bundle": "^2.1",
		"imagine/imagine": "^0.6",
		"intervention/image": "^2.7",
		"league/commonmark": "^2.4",
		"league/html-to-markdown": "^5.1",
		"mollie/mollie-api-php": "^2",
		"monolog/monolog": "^1",
		"nyholm/psr7": "^1.5",
		"ocramius/proxy-manager": "^2.10",
		"php-http/guzzle6-adapter": "^1",
		"php-http/message": "^1.6",
		"psr/http-client": "^1.0",
		"sensio/framework-extra-bundle": "^6",
		"snc/redis-bundle": "^4.1",
		"symfony/amazon-mailer": "^6",
		"symfony/asset": "^6",
		"symfony/cache": "^6",
		"symfony/config": "^6",
		"symfony/console": "^6",
		"symfony/contracts": "^3",
		"symfony/debug-bundle": "^6",
		"symfony/dotenv": "^6",
		"symfony/expression-language": "^6",
		"symfony/finder": "^6",
		"symfony/flex": "^2",
		"symfony/form": "^6",
		"symfony/framework-bundle": "^6",
		"symfony/html-sanitizer": "^6",
		"symfony/http-client": "^6",
		"symfony/mailer": "^6",
		"symfony/messenger": "^6",
		"symfony/mime": "^6",
		"symfony/monolog-bridge": "^6",
		"symfony/monolog-bundle": "^3",
		"symfony/process": "^6",
		"symfony/proxy-manager-bridge": "^6",
		"symfony/requirements-checker": "^2.0",
		"symfony/runtime": "^6",
		"symfony/security-bundle": "^6",
		"symfony/security-csrf": "^6",
		"symfony/serializer": "^6",
		"symfony/stopwatch": "^6",
		"symfony/translation": "^6",
		"symfony/twig-bridge": "^6",
		"symfony/twig-bundle": "^6",
		"symfony/validator": "^6",
		"symfony/var-dumper": "^6",
		"symfony/yaml": "^6",
		"twig/cssinliner-extra": "^3.5",
		"twig/extra-bundle": "^3.5",
		"twig/inky-extra": "^3.5",
		"twig/markdown-extra": "^3.5",
		"twig/twig": "^2.12|^3.0",
		"xemlock/htmlpurifier-html5": "^0.1.11"
	},

    "require-dev": {
		"symfony/maker-bundle": "^1.13",
		"symfony/phpunit-bridge": "^6.0",
		"symfony/web-profiler-bundle": "^6"
	},

    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
	},

    "minimum-stability": "stable",

	"autoload": {
		"psr-4": {
			"App\\": "src/"
        }
	},

    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
	},

    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
	},

    "scripts": {
        "auto-scripts": {
			"cache:clear": "symfony-cmd",
			"assets:install %PUBLIC_DIR%": "symfony-cmd",
			"requirements-checker": "script"
		},
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
	},

    "conflict": {
        "symfony/symfony": "*"
	},

    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6"
        }
    }
}
