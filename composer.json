{
	"name": "eeemarv/eland",
	"license": "AGPL-3.0",
	"type": "project",
	"description": "web app for community currencies.",
	"homepage": "https://eland.letsa.net",
	"keywords": ["community", "currencies", "app"],

    "require": {
		"php": "^7.3",
		"aws/aws-sdk-php": "^3",
		"swiftmailer/swiftmailer": "~6",
		"monolog/monolog": "^1",
		"bramus/monolog-colored-line-formatter": "2.0.*",
		"econea/nusoap": "^v0.9.5.3",
		"fabpot/goutte": "^3",
		"ext-imagick": "*",
		"ext-exif": "*",
		"ext-gd": "*",
		"ext-mbstring": "*",
        "imagine/imagine": "^0.6",
        "doctrine/dbal": "^2.5",
        "twig/twig": "^2",
        "ezyang/htmlpurifier": "^4.8",
		"league/html-to-markdown": "^4.4",
		"symfony/twig-bridge": "^4",
		"symfony/security": "^4",
		"symfony/finder": "^4",
		"predis/service-provider": "^2.0",
		"symfony/form": "^4",
		"symfony/validator": "^4",
		"symfony/process": "^4",
		"symfony/config": "^4.2",
		"symfony/security-csrf": "^4",
		"symfony/monolog-bridge": "^4",
		"symfony/translation": "^4",
		"symfony/asset": "^4",
		"geocoder-php/google-maps-provider": "^4.1",
		"php-http/guzzle6-adapter": "^1.1",
		"php-http/message": "^1.6",
		"symfony/contracts": "^1.1",
		"gregwar/captcha": "^1.1",
		"symfony/var-dumper": "^4.3",
		"mollie/mollie-api-php": "^2.12",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.3.*",
        "symfony/yaml": "4.3.*"
	},

    "require-dev": {
	},

    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
	},

    "minimum-stability": "stable",

	"autoload": {
		"psr-4": {
			"App\\": "src/",
			"service\\": "service/",
			"model\\": "model/",
			"queue\\": "queue/",
			"task\\": "task/",
			"schema_task\\": "schema_task/",
			"util\\": "util/",
			"twig\\": "twig/",
			"render\\": "render/",
			"cnst\\": "cnst/",
			"command\\": "command/",
			"controller\\": "controller"
        }
	},

    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
	},

    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
	},

    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
	},

    "conflict": {
        "symfony/symfony": "*"
	},

    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.3.*"
        }
    }
}
