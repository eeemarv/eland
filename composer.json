{
	"name": "eeemarv/eland",
	"license": "AGPL-3.0",
	"type": "project",
	"description": "web app for community currencies.",
	"homepage": "https://eland.letsa.net",
	"keywords": ["community", "currencies", "app"],

    "require": {
		"php": "^7.4",
		"ext-ctype": "*",
		"ext-exif": "*",
		"ext-gd": "*",
		"ext-iconv": "*",
		"ext-imagick": "*",
		"ext-mbstring": "*",
		"ext-redis": "*",
		"aws/aws-sdk-php": "^3",
		"bramus/monolog-colored-line-formatter": "2.0.*",
		"doctrine/annotations": "^1.10",
		"doctrine/dbal": "^2.5",
		"ezyang/htmlpurifier": "^4.8",
		"fabpot/goutte": "^3",
		"geocoder-php/google-maps-provider": "^4.1",
		"gregwar/captcha-bundle": "^2.1",
		"imagine/imagine": "^0.6",
		"league/html-to-markdown": "^4.4",
		"minishlink/web-push-bundle": "^5.0",
		"mollie/mollie-api-php": "^2",
		"monolog/monolog": "^1",
		"php-http/guzzle6-adapter": "^1.1",
		"php-http/message": "^1.6",
		"predis/predis": "^1.1",
		"sensio/framework-extra-bundle": "^5.4",
		"swiftmailer/swiftmailer": "~6",
		"symfony/asset": "^5",
		"symfony/cache": "^5",
		"symfony/config": "^5",
		"symfony/console": "^5",
		"symfony/contracts": "^1.1",
		"symfony/debug-pack": "^1.0",
		"symfony/dotenv": "^5",
		"symfony/expression-language": "5.0.*",
		"symfony/finder": "^5",
		"symfony/flex": "^1.3.1",
		"symfony/form": "^5",
		"symfony/framework-bundle": "^5",
		"symfony/messenger": "^5",
		"symfony/monolog-bridge": "^5",
		"symfony/monolog-bundle": "^3",
		"symfony/orm-pack": "^1.0",
		"symfony/process": "^5",
		"symfony/profiler-pack": "^1.0",
		"symfony/proxy-manager-bridge": "^5",
		"symfony/security-bundle": "^5",
		"symfony/security-csrf": "^5",
		"symfony/translation": "^5",
		"symfony/twig-bridge": "^5",
		"symfony/twig-bundle": "^5",
		"symfony/twig-pack": "^1.0",
		"symfony/validator": "^5",
		"symfony/var-dumper": "^5.0",
		"symfony/webpack-encore-bundle": "^1.7",
		"symfony/yaml": "5.0.*",
		"twig/twig": "^3"
	},

    "require-dev": {
		"symfony/maker-bundle": "^1.13"
	},

    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
	},

    "minimum-stability": "stable",

	"autoload": {
		"psr-4": {
			"App\\": "src/"
        }
	},

    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
	},

    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
	},

    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
	},

    "conflict": {
        "symfony/symfony": "*"
	},

    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    }
}
