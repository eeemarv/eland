{
	"name": "eeemarv/eland",
	"license": "AGPL-3.0",
	"type": "project",
	"description": "web app for community currencies.",
	"homepage": "https://eland.letsa.net",
	"keywords": ["community", "currencies", "app"],

    "require": {
		"php": "^8.3",
		"ext-ctype": "*",
		"ext-exif": "*",
		"ext-gd": "*",
		"ext-iconv": "*",
		"ext-intl": "*",
		"ext-mbstring": "*",
		"ext-redis": "*",
		"ext-xml": "*",
		"ext-zip": "*",
		"aws/aws-sdk-php": "^3",
		"bramus/monolog-colored-line-formatter": "~3.1",
		"composer/package-versions-deprecated": "1.11.99.1",
		"doctrine/dbal": "^3",
		"doctrine/doctrine-bundle": "^2",
		"doctrine/doctrine-migrations-bundle": "^3",
		"doctrine/orm": "^2",
		"geo6/geocoder-php-bpost-provider": "^1.4",
		"imagine/imagine": "^0.6",
		"intervention/image": "^2.7",
		"league/commonmark": "^2.4",
		"league/html-to-markdown": "^5.1",
		"mollie/mollie-api-php": "^2",
		"monolog/monolog": "^3",
		"nyholm/psr7": "^1.5",
		"ocramius/proxy-manager": "^2.10",
		"php-http/guzzle6-adapter": "^1",
		"php-http/message": "^1.6",
		"psr/http-client": "^1.0",
		"psr/http-factory": "^1.0",
		"snc/redis-bundle": "^4.1",
		"symfony/amazon-mailer": "7.0.*",
		"symfony/asset": "7.0.*",
		"symfony/cache": "7.0.*",
		"symfony/config": "7.0.*",
		"symfony/console": "7.0.*",
		"symfony/contracts": "^3",
		"symfony/debug-bundle": "7.0.*",
		"symfony/dotenv": "7.0.*",
		"symfony/expression-language": "7.0.*",
		"symfony/finder": "7.0.*",
		"symfony/flex": "^2",
		"symfony/form": "7.0.*",
		"symfony/framework-bundle": "7.0.*",
		"symfony/html-sanitizer": "7.0.*",
		"symfony/http-client": "7.0.*",
		"symfony/mailer": "7.0.*",
		"symfony/messenger": "7.0.*",
		"symfony/mime": "7.0.*",
		"symfony/monolog-bridge": "7.0.*",
		"symfony/monolog-bundle": "^3",
		"symfony/process": "7.0.*",
		"symfony/proxy-manager-bridge": "6.4.*",
		"symfony/requirements-checker": "^2.0",
		"symfony/runtime": "7.0.*",
		"symfony/security-bundle": "7.0.*",
		"symfony/security-csrf": "7.0.*",
		"symfony/serializer": "7.0.*",
		"symfony/stopwatch": "7.0.*",
		"symfony/translation": "7.0.*",
		"symfony/twig-bridge": "7.0.*",
		"symfony/twig-bundle": "7.0.*",
		"symfony/validator": "7.0.*",
		"symfony/var-dumper": "7.0.*",
		"symfony/yaml": "7.0.*",
		"twig/cssinliner-extra": "^3.10",
		"twig/extra-bundle": "^3.10",
		"twig/inky-extra": "^3.5",
		"twig/markdown-extra": "^3.5",
		"twig/twig": "^3.0"
	},

    "require-dev": {
		"symfony/maker-bundle": "^1.13",
		"symfony/phpunit-bridge": "^7.0",
		"symfony/web-profiler-bundle": "7.0.*"
	},

    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
	},

    "minimum-stability": "stable",

	"autoload": {
		"psr-4": {
			"App\\": "src/"
        }
	},

    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
	},

    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
	},

    "scripts": {
        "auto-scripts": {
			"cache:clear": "symfony-cmd",
			"assets:install %PUBLIC_DIR%": "symfony-cmd",
			"requirements-checker": "script"
		},
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
	},

    "conflict": {
        "symfony/symfony": "*"
	},

    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.0.*"
        }
    }
}
