{
	"name": "eeemarv/eland",
	"license": "AGPL-3.0",
	"type": "project",
	"description": "web app for community currencies.",
	"homepage": "https://eland.letsa.net",
	"keywords": ["community", "currencies", "app"],

    "require": {
		"php": "^8.0",
		"ext-xml": "*",
		"ext-ctype": "*",
		"ext-exif": "*",
		"ext-gd": "*",
		"ext-iconv": "*",
		"ext-intl": "*",
		"ext-mbstring": "*",
		"ext-redis": "*",
		"ext-zip": "*",
		"aws/aws-sdk-php": "^3",
		"bramus/monolog-colored-line-formatter": "^2",
		"composer/package-versions-deprecated": "1.11.99.1",
		"doctrine/dbal": "^2",
		"doctrine/doctrine-bundle": "^2",
		"doctrine/doctrine-migrations-bundle": "^3",
		"doctrine/orm": "^2",
		"ezyang/htmlpurifier": "^4.8",
		"fabpot/goutte": "^3",
		"geo6/geocoder-php-bpost-provider": "^1.3",
		"gregwar/captcha": "^1.1",
		"gregwar/captcha-bundle": "^2.1",
		"imagine/imagine": "^0.6",
		"intervention/image": "^2.5",
		"league/html-to-markdown": "^4.4",
		"mollie/mollie-api-php": "^2",
		"monolog/monolog": "^1",
		"ocramius/proxy-manager": "^2.10",
		"php-http/guzzle6-adapter": "^1.1",
		"php-http/message": "^1.6",
		"predis/predis": "1.1.4",
		"sensio/framework-extra-bundle": "^5",
		"swiftmailer/swiftmailer": "^6",
		"symfony/asset": "^5",
		"symfony/cache": "^5",
		"symfony/config": "^5",
		"symfony/console": "^5",
		"symfony/contracts": "^2",
		"symfony/dotenv": "^5",
		"symfony/finder": "^5",
		"symfony/flex": "^1.3.1",
		"symfony/form": "^5",
		"symfony/framework-bundle": "^5",
		"symfony/mime": "^5",
		"symfony/monolog-bridge": "^5",
		"symfony/monolog-bundle": "^3",
		"symfony/process": "^5",
		"symfony/proxy-manager-bridge": "^5",
		"symfony/requirements-checker": "^2.0",
		"symfony/security-bundle": "^5",
		"symfony/security-csrf": "^5",
		"symfony/translation": "^5",
		"symfony/twig-bridge": "^5",
		"symfony/twig-bundle": "^5",
		"symfony/validator": "^5",
		"symfony/var-dumper": "^5",
		"symfony/yaml": "^5",
		"twig/extra-bundle": "^2.12|^3.0",
		"twig/twig": "^3",
		"xemlock/htmlpurifier-html5": "^0.1.11"
	},

    "require-dev": {
		"symfony/debug-bundle": "^5",
		"symfony/maker-bundle": "^1.13",
		"symfony/stopwatch": "^5",
		"symfony/web-profiler-bundle": "^5"
	},

    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
	},

    "minimum-stability": "stable",

	"autoload": {
		"psr-4": {
			"App\\": "src/"
        }
	},

    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
	},

    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
	},

    "scripts": {
        "auto-scripts": {
			"cache:clear": "symfony-cmd",
			"assets:install %PUBLIC_DIR%": "symfony-cmd",
			"requirements-checker": "script"
		},
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
	},

    "conflict": {
        "symfony/symfony": "*"
	},

    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5"
        }
    }
}
