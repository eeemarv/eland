{%- extends 'contacts/base/contacts_base.html.twig' -%}

{%- block asssets_add -%}
    {{- parent() -}}
    {{- assets_add(['table_sel.js']) -}}
{%- endblock asssets_add -%}

{%- block btn_top_add -%}
    {%- include 'include/btn_top/add.html.twig' with {
        'btn_route': 'contacts_add',
        'btn_title': 'contacts_add.btn_add'
    } -%}
{%- endblock btn_top_add -%}

{%- block btn_top_bulk -%}
    {%- include 'include/btn_top/bulk.html.twig' -%}
{%- endblock btn_top_bulk -%}

{%- set csv_en = true -%}

{%- block heading_title -%}
    {{- 'contacts.title'|trans -}}
{%- endblock heading_title -%}

{%- block heading_append -%}
    {%- include 'include/heading_filter.html.twig' -%}
    {%- include 'include/heading_btn_filter.html.twig' -%}
{%- endblock heading_append -%}

{%- block css -%}
    {{- parent() -}}
    {{- encore_entry_link_tags('footable') -}}
{%- endblock css -%}

{%- block js -%}
    {{- parent() -}}
	{{- encore_entry_script_tags('footable') -}}
{%- endblock js -%}