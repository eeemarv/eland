{%- extends 'news/base/news_overview.html.twig' -%}

{%- set csv_en = true -%}

{%- block data -%}
    {%- if news_items -%}
        {%- include 'news/include/news_list.html.twig' -%}
    {%- else -%}
        {%- include 'include/no_results.html.twig' with {
            'message': 'news.no_items'
        } -%}
    {%- endif -%}
{%- endblock data -%}

{%- block css -%}
    {{- parent() -}}
    {{- encore_entry_link_tags('footable') -}}
{%- endblock css -%}

{%- block js -%}
    {{- parent() -}}
    {{- encore_entry_script_tags('footable') -}}
{%- endblock js -%}