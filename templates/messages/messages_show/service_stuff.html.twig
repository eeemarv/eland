{%- if config_bool('messages.fields.service_stuff.enabled', schema) -%}
{%- embed 'embed/list_item.html.twig' -%}
    {%- import 'macro/lbl.html.twig' as l -%}
    {%- block dl -%}
        <dt>
            {{- 'label.services_stuff'|trans -}}
        </dt>
        <dd>
            {%- if message.service_stuff is not null and message.service_stuff != '' -%}
                {{- l.label_lg(message.service_stuff) -}}
            {%- else -%}
                {{- null|no_content -}}
            {%- endif -%}
        </dd>
    {%- endblock dl -%}
    {%- block div_right -%}
        {%- if pp_is_admin() or su_is_owner(user.id) -%}
            {%- include 'include/btn/edit.html.twig' with {
                'btn_route': 'messages_service_stuff_edit',
                'btn_route_params': {'id': id},
                'btn_title': 'messages_service_stuff_edit.btn',
                'btn_label_hidden': 'sm',
                'btn_lg': true,
            } -%}
        {%- endif -%}
    {%- endblock div_right -%}
{%- endembed -%}
{%- endif -%}
