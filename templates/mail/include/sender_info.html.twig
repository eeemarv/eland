{%- trans_default_domain 'mail' -%}

{%- embed 'mail/embed/table600.html.twig' -%}
    {%- trans_default_domain 'mail' -%}
    {%- block td_top -%}20px{%- endblock -%}
    {%- block td_bottom -%}20px{%- endblock -%}
    {%- block bgcolor_attr -%}{%- endblock -%}
    {%- block td_align -%}left{%- endblock -%}
    {%- block td_style %}font-size: 16px;{%- endblock -%}
    {%- block td_content -%}
    <table border="1" bgcolor="eeeeee" cellpadding="0" cellspacing="0" width="100%">
        <tr>
            <td width="150" style="padding-left: 10px">
                {{- 'label.email'|trans -}}
            </td>
            <td style="font-style: bold; padding-left: 10px">
                {{- email -}}
            </td>
        </tr>
        <tr>
            <td width="150" style="padding-left: 10px">
                {{- 'label.ip'|trans -}}
            </td>
            <td style="font-style: bold; padding-left: 10px">
                {{- ip -}}
            </td>
        </tr>
        <tr>
            <td width="150" style="padding-left: 10px">
                {{- 'label.agent'|trans -}}
            </td>
            <td style="font-style: bold; padding-left: 10px">
                {{- agent -}}
            </td>
        </tr>
    </table>
    {%- endblock td_content -%}
{%- endembed -%}

{%- include 'mail/include/bgspacer40.html.twig' -%}
