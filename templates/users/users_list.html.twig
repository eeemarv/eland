{%- extends 'users/base/users_overview.html.twig' -%}

{%- set csv_en = true -%}

{%- block assets_add -%}
    {{- parent() -}}
    {{- assets_add([
        'calc_sum.js',
        'users_distance.js',
        'datepicker'
    ]) -}}
    {%- if pp_is_admin() -%}
        {{- assets_add([
            'codemirror',
            'summernote',
            'summernote_email.js',
            'table_sel.js',
        ]) -}}
    {%- endif -%}
{%- endblock assets_add -%}

{%- block btn_show_columns -%}
    {%- include 'include/nav/btn_show_columns.html.twig' -%}
{%- endblock btn_show_columns -%}

{%- block btn_top_bulk -%}
    {%- if pp_is_admin() -%}
        {%- include 'include/btn_top/bulk.html.twig' -%}
    {%- endif -%}
{%- endblock btn_top_bulk -%}

{%- block columns_form -%}
    {{- columns_form_raw|raw -}}
{%- endblock columns_form -%}

{%- block filter_form_a -%}
    {%- include 'form/q_filter.html.twig' -%}
{%- endblock filter_form_a -%}

{%- block css -%}
    {{- parent() -}}
    {{- encore_entry_link_tags('footable') -}}
{%- endblock css -%}

{%- block js -%}
    {{- parent() -}}
	{{- encore_entry_script_tags('footable') -}}
	{{- encore_entry_script_tags('typeahead') -}}
{%- endblock js -%}