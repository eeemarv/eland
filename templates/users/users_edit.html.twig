{%- extends 'users/base/users_edit_form.html.twig' -%}

{%- block heading_title -%}
    {%- if is_self -%}
        {{- 'users_edit_self.title'|trans -}}
    {%- else -%}
        {{- 'users_edit.title'|trans({
            'user': account(id, schema)|link('users_show', {'id': id})
        })|raw -}}
    {%- endif -%}
{%- endblock heading_title -%}
